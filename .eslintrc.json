{
    "env": {
        "browser": true,
        "es2020": true,
        "node": true
    },
    "extends": [
        "google",
        "plugin:react/recommended",
        "prettier"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": {
        "prettier/prettier": "warn",
        "require-jsdoc": 0,
        "react/display-name": 0,
        "guard-for-in": "off",
        "react/prop-types": "off"
    }
}